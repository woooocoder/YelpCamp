
<% if (success && success.length) { %>
<div class="alert alert-success alert-dismissable fade show" role="alert" aria-labe="Close">
    <%= success %>
    <button class="close" type="button" data-dismiss="alert">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<% } %>


<% if (error && error.length) { %>
    <div class="alert alert-danger alert-dismissable fade show" role="alert" aria-labe="Close">
        <%= error %>
        <button class="close" type="button" data-dismiss="alert">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <% } %>